{
  "name" : "emi",
  "type" : "bin",
  "clear_build_path" : true,
  "build_in" : "github",
  "next": [
    {
      "build" : "py",
      "copy" : [
        {"../../src/" : "src/"},
        {"../../inc/" : "inc/"},
        {"../../test/" : "test/"},
        {"../../README.md" : "README.md"},
        {"../../LICENSE" : "LICENSE"}
      ],
      "config" : {
        "author_name" : "eons",
        "author_email" : "support@eons.llc",
        "description" : "eons Modular Installer",
        "package_url" : "https://github.com/eons-dev/bin_emi",
        "license" : "MIT License",
        "python_min" : "3.7",
        "classifiers" : [
          "Development Status :: 3 - Alpha"
        ]
      }
    }
  ]
}
